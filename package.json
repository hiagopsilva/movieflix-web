{
  "name": "MovieFlix",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "5.0.0-beta.5",
    "@material-ui/icons": "^4.11.2",
    "@sentry/react": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.4",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "mobx": "6.3.2",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^7.1.0",
    "mobx-react-router": "^4.1.0",
    "polished": "^4.2.2",
    "react": "^17.0.2",
    "react-code-input": "^3.10.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^5.3.1",
    "react-toastify": "^8.0.3",
    "recharts": "^2.1.9",
    "styled-components": "^5.0.1",
    "typescript": "^4.4.3",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev": "env-cmd -f .env.dev yarn react-app-rewired start",
    "hml": "env-cmd -f .env.hml yarn react-app-rewired start",
    "prd": "env-cmd -f .env.prd yarn react-app-rewired start",
    "build:dev": "env-cmd -f .env.dev yarn react-app-rewired build",
    "build:hml": "env-cmd -f .env.hml yarn react-app-rewired build",
    "build:prd": "env-cmd -f .env.prd yarn react-app-rewired build",
    "build": "env-cmd -f .env.prd yarn react-app-rewired build",
    "test": "react-app-rewired test",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --config ./.prettierrc --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc && eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js, jsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.175",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "commitlint": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "react-app-rewired": "^2.1.8",
    "reactotron-react-js": "^3.3.7"
  }
}
